vider></v-divider>\n\n        <v-stepper-step\n            :complete=\"stepper > 2\"\n            step=\"2\"\n        >\n          Patient Vitals\n        </v-stepper-step>\n\n        <v-divider></v-divider>\n\n        <v-stepper-step step=\"3\" :complete=\"stepper > 3\">\n          Patient Vitals Form A\n        </v-stepper-step>\n        <v-divider/>\n\n        <v-stepper-step step=\"4\" :complete=\"stepper > 4\">\n          Patient Vitals Form B\n        </v-stepper-step>\n        <v-divider/>\n\n        <v-stepper-step step=\"5\">\n          Patient Listing\n        </v-stepper-step>\n      </v-stepper-header>\n\n      <v-stepper-items>\n        <v-stepper-content step=\"1\">\n          <v-card flat>\n            <v-form ref=\"registrationForm\">\n              <v-row>\n                <v-col cols=\"12\" md=\"6\">\n                  <v-text-field\n                      label=\"Patient Number\"\n                      outlined\n                      v-model=\"patientFormData.patientID\"\n                      dense\n                  />\n                </v-col>\n                <v-col cols=\"12\" md=\"6\">\n                  <v-text-field\n                      type=\"date\"\n                      label=\"Registration Date\"\n                      v-model=\"patientFormData.registrationDate\"\n                      outlined\n                      dense\n                  />\n                </v-col>\n                <v-col cols=\"12\" md=\"6\">\n                  <v-text-field\n                      label=\"First Name\"\n                      outlined\n                      v-model=\"patientFormData.firstName\"\n                      dense\n                  />\n                </v-col>\n                <v-col cols=\"12\" md=\"6\">\n                  <v-text-field\n                      label=\"Last Name\"\n                      outlined\n                      v-model=\"patientFormData.lastName\"\n                      dense\n                  />\n                </v-col>\n                <v-col cols=\"12\" md=\"6\">\n                  <v-text-field\n                      label=\"Date of Birth\"\n                      outlined\n                      dense\n                      type=\"date\"\n                      v-model=\"patientFormData.dateOfBirth\"\n                  />\n                </v-col>\n                <v-col cols=\"12\" md=\"6\">\n                  <v-select\n                      outlined\n                      dense\n                      label=\"Gender\"\n                      v-model=\"patientFormData.gender\"\n                      :items=\"gender\"\n                      :item-text=\"item=>item.value\"\n                      :item-value=\"item=>item.value\"\n                  />\n                </v-col>\n              </v-row>\n            </v-form>\n          </v-card>\n\n          <v-btn\n              color=\"primary\"\n              @click=\"savePatient\"\n          >\n            Continue\n          </v-btn>\n\n          <v-btn text>\n            Cancel\n          </v-btn>\n        </v-stepper-content>\n\n        <v-stepper-content step=\"2\">\n          <v-card flat>\n            <v-form ref=\"vitalsForm\">\n              <v-row>\n                <v-col cols=\"12\" md=\"6\">\n                  <v-text-field\n                      label=\"Patient Name\"\n                      outlined\n                      readonly\n                      :value=\"`${patientFormData.firstName+' '+ patientFormData.lastName}`\"\n                      dense\n                  />\n                </v-col>\n                <v-col cols=\"12\" md=\"6\">\n                  <v-text-field\n                      type=\"date\"\n                      label=\"Visit Date\"\n                      outlined\n                      v-model=\"vitalsFormData.visitDate\"\n                      dense\n                  />\n                </v-col>\n                <v-col cols=\"12\" md=\"6\">\n                  <v-text-field\n                      label=\"Height\"\n                      outlined\n                      type=\"number\"\n                      v-model=\"vitalsFormData.height\"\n                      dense\n                  />\n                </v-col>\n                <v-col cols=\"12\" md=\"6\">\n                  <v-text-field\n                      label=\"Weight\"\n                      outlined\n                      type=\"number\"\n                      v-model=\"vitalsFormData.weight\"\n                      dense\n                  />\n                </v-col>\n                <v-col cols=\"12\" md=\"6\">\n                  <v-text-field\n                      label=\"BMI\"\n                      outlined\n                      readonly\n                      type=\"number\"\n                      :value=\"`${vitalsFormData.weight/vitalsFormData.height}`\"\n                      v-model=\"vitalsFormData.BMI\"\n                      dense\n                  />\n                </v-col>\n              </v-row>\n            </v-form>\n          </v-card>\n\n          <v-btn\n              color=\"primary\"\n              @click=\"saveVitals\"\n          >\n            Continue\n          </v-btn>\n\n          <v-btn text>\n            Cancel\n          </v-btn>\n        </v-stepper-content>\n\n        <v-stepper-content step=\"3\">\n          <v-card flat>\n            <v-form ref=\"formA\">\n              <v-row>\n                <v-col cols=\"12\" md=\"6\">\n                  <v-text-field\n                      label=\"Patient Name\"\n                      outlined\n                      readonly\n                      :value=\"`${patientFormData.firstName+' '+ patientFormData.lastName}`\"\n                      dense\n                  />\n                </v-col>\n                <v-col cols=\"12\" md=\"6\">\n                  <v-text-field\n                      type=\"date\"\n                      label=\"Visit Date\"\n                      outlined\n                      v-model=\"visitFormData.dietVisitDate\"\n                      dense\n                  />\n                </v-col>\n                <v-col cols=\"12\" md=\"6\">\n                  <p>General Health</p>\n                  <v-radio-group>\n                    <v-radio\n                        :label=\"val.value\"\n                        :value=\"val.value\"\n                        v-model=\"visitFormData.dietGeneralHealth\"\n                        v-bind:key=\"index\"\n                        v-for=\"(val, index) in health\"></v-radio>\n                  </v-radio-group>\n                </v-col>\n                <v-col cols=\"12\" md=\"6\">\n                  <p>Have you ever been on a diet to lose weight?</p>\n                  <v-radio-group>\n                    <v-radio\n                        :label=\"yes.value\"\n                        :value=\"yes.value\"\n                        v-bind:key=\"index\"\n                        v-model=\"visitFormData.isOnDietToLoseWeight\"\n                        v-for=\"(yes, index) in yesOrNo\"></v-radio>\n                  </v-radio-group>\n                </v-col>\n                <v-col cols=\"12\">\n                  <v-textarea\n                      label=\"Comments\"\n                      outlined\n                      v-model=\"visitFormData.dietComments\"\n                  />\n                </v-col>\n              </v-row>\n            </v-form>\n          </v-card>\n\n          <v-btn\n              color=\"primary\"\n              @click=\"stepper = 4\"\n          >\n            Continue\n          </v-btn>\n\n          <v-btn text>\n            Cancel\n          </v-btn>\n        </v-stepper-content>\n        <v-stepper-content step=\"4\">\n          <v-card flat>\n            <v-form ref=\"formB\">\n              <v-row>\n                <v-col cols=\"12\" md=\"6\">\n                  <v-text-field\n                      label=\"Patient Name\"\n                      outlined\n                      dense\n                      readonly\n                      :value=\"`${patientFormData.firstName+' '+ patientFormData.lastName}`\"\n                  />\n                </v-col>\n                <v-col cols=\"12\" md=\"6\">\n                  <v-text-field\n                      type=\"date\"\n                      label=\"Visit Date\"\n                      outlined\n                      v-model=\"visitFormData.drugsVisitDate\"\n                      dense\n                  />\n                </v-col>\n                <v-col cols=\"12\" md=\"6\">\n                  <p>General Health</p>\n                  <v-radio-group>\n                    <v-radio\n                        :label=\"val.value\"\n                        :value=\"val.value\"\n                        v-model=\"visitFormData.isOnDrugs\"\n                        v-bind:key=\"index+1\"\n                        v-for=\"(val, index) in health\"></v-radio>\n                  </v-radio-group>\n                </v-col>\n                <v-col cols=\"12\" md=\"6\">\n                  <p>Are you currently taking any drugs?</p>\n                  <v-radio-group>\n                    <v-radio\n                        v-model=\"visitFormData.drugsGeneralHealth\"\n                        :label=\"yes.value\" :value=\"yes.value\"\n                        v-bind:key=\"index+2\" v-for=\"(yes, index) in yesOrNo\"></v-radio>\n                  </v-radio-group>\n                </v-col>\n                <v-col cols=\"12\">\n                  <v-textarea\n                      label=\"Comments\"\n                      outlined\n                      v-model=\"visitFormData.drugsComments\"\n                  />\n                </v-col>\n              </v-row>\n            </v-form>\n          </v-card>\n\n          <v-btn\n              color=\"primary\"\n              @click=\"saveVisits\"\n          >\n            Continue\n          </v-btn>\n\n          <v-btn text>\n            Cancel\n          </v-btn>\n        </v-stepper-content>\n        <v-stepper-content step=\"5\">\n          <v-card>\n            <v-data-table\n                :items=\"patients\"\n                :headers=\"headers\"\n            />\n          </v-card>\n\n          <v-btn\n              color=\"primary\"\n              @click=\"stepper = 1\"\n          >\n            Continue\n          </v-btn>\n\n          <v-btn text>\n            Cancel\n          </v-btn>\n        </v-stepper-content>\n      </v-stepper-items>\n    </v-stepper>\n  </v-container>\n</template>\n\n<script>\nexport default {\n  name: \"RegisterComponent\",\n  beforeRouteEnter(to,from,next){\n    next((v) =>{\n      v.$store.dispatch('patients')\n    })\n  },\n  data () {\n    return {\n      patientFormData:{\n        patientID: \"\",\n        registrationDate: \"\",\n        firstName: \"\",\n        lastName: \"\",\n        dateOfBirth: \"\",\n        gender: \"\",\n      },\n      vitalsFormData: {\n        patientID: \"\",\n        visitDate: \"\",\n        height: null,\n        weight: null,\n        BMI: null,\n      },\n      visitFormData: {\n        patientID: \"\",\n        dietVisitDate: \"\",\n        drugsVisitDate: \"\",\n        dietGeneralHealth: \"\",\n        drugsGeneralHealth: \"\",\n        isOnDietToLoseWeight: \"\",\n        isOnDrugs: \"\",\n        dietComments: \"\",\n        drugsComments: \"\",\n      },\n      stepper: 1,\n      gender: [\n        {\"value\":\"Male\"},\n        {\"value\":\"Female\"},\n        {\"value\":\"Other\"},\n      ],\n      yesOrNo: [\n        {\"value\":\"Yes\"},\n        {\"value\":\"No\"},\n      ],\n      health: [\n        {\"value\":\"Good\"},\n        {\"value\":\"Poor\"},\n      ],\n      headers: [\n        {text: 'Patient Name', value: 'first_name'},\n        {text: 'Age', value: 'patient_number'},\n        {text: 'BMI Status', value: 'gender'}\n      ],\n    }\n  },\n  computed:{\n    patient(){\n      return this.$store.getters['patientGetter']('patient')\n    },\n    patients(){\n      return this.$store.getters['patientGetter']('patients')\n    }\n  },\n  methods: {\n    savePatient() {\n      this.stepper = 2\n      this.$store.dispatch('savePatient', {...this.patientFormData})\n    },\n    saveVitals() {\n      this.stepper = 3\n      this.vitalsFormData.patientID=this.patientFormData.patientID;\n      this.vitalsFormData.BMI=this.vitalsFormData.weight/this.vitalsFormData.height\n      this.$store.dispatch('saveVitals', {...this.vitalsFormData})\n    },\n    saveVisits() {\n      this.stepper = 5\n      this.visitFormData.patientID=this.patientFormData.patientID;\n      this.$store.dispatch('saveVisits', {...this.visitFormData})\n    }\n  }\n}\n</script>\n\n<style scoped>\n@import url('https://fonts.googleapis.com/css2?family=Baloo+2&family=PT+Sans+Caption&display=swap');\n*{\n  font-family: 'PT Sans Caption', sans-serif;\n}\n</style>\n"],"sourceRoot":"src/components"}]}